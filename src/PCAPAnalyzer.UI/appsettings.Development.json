{
    "Analysis":  {
                     "MaxCacheSize":  "100MB",
                     "EnableRealTimeAnalysis":  true,
                     "CacheResults":  true
                 },
    "TShark":  {
                   "MaxConcurrentProcesses":  4,
                   "TimeoutSeconds":  30,
                   "ExecutablePath":  "tshark"
               },
    "CsvExport": {
                     "AllowedDirectories": [
                         "%USERPROFILE%\\Documents\\PCAPAnalyzer_Dev",
                         "%USERPROFILE%\\Desktop",
                         "%TEMP%"
                     ],
                     "MaxExportRows": 1000000,
                     "EnableSecurityAuditLog": true
                 },
    "Logging":  {
                    "LogLevel":  {
                                     "Microsoft.Hosting.Lifetime":  "Information",
                                     "Microsoft":  "Warning",
                                     "Default":  "Debug",
                                     "PCAPAnalyzer.UI.Services.CsvExportService":  "Debug"
                                 }
                }
}
